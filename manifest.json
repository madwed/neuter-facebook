{
	"manifest_version": 2,

	"name": "Neuter Facebook",
	"description": "This extension blocks distracting elements on Facebook",
	"version": "0.1",

	"browser_action": {
		"default_title": "Click here!",
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"activeTab",
		"tabs",
		"https://ajax.googleapis.com/",
		"https://facebook.com/"
	]
}