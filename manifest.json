{
	"manifest_version": 2,

	"name": "Neuter Facebook",
	"description": "This extension blocks distracting elements on Facebook",
	"version": "0.1",

	"content_scripts": [
		{
			"matches": ["*://*.facebook.com/*"],
			"css": ["neuter.css"],
			"js": ["neuter.js"]

		}
	],
	
	"permissions": [
		"activeTab",
		"tabs"
	]
}
